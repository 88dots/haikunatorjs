{
  "name": "haikunator",
  "version": "1.0.2",
  "description": "Generate Heroku-like random names to use in your node applications.",
  "main": "lib/haikunator.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "./node_modules/.bin/babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Atrox/haikunatorjs"
  },
  "keywords": [
    "haikunator",
    "heroku",
    "random",
    "names",
    "heroku-like",
    "name",
    "generator"
  ],
  "author": "Atrox",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Atrox/haikunatorjs/issues"
  },
  "homepage": "https://github.com/Atrox/haikunatorjs",
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.3.0",
    "coveralls": "^2.11.4",
    "isparta": "^3.0.4",
    "mocha": "^2.3.3"
  }
}
